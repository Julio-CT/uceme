@model IEnumerable<Uceme.Model.Models.ClasesVista.BlogVista>

@{
    ViewBag.Title = "Articulos Publicados - UCEME";

}

@*<h2 class="titulo">Artículos de interes para el pacientes y profesionales</h2>*@
<br />

@if (Request.IsAuthenticated)
{
    <div class="botones" style="float: right;">
        <input type="button" class="nuevo" name="addSer" onclick="manager.anadir()" />
    </div>
    <br />
}
<div id="contenedor">
@foreach (var item in Model)
    {
        @Html.DisplayFor(modelItem => item, "ArticuloVista")
    }
</div>
<div id="loading"></div>

@section scripts {
    <script type="text/javascript" src = "/Scripts/Internal/Articulos/Index.js"async></script>
}