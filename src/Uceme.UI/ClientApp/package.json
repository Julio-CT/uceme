    "html-react-parser": "^1.4.12",
    "html-to-draftjs": "^1.5.0",
    "http-proxy-middleware": "^1.0.6",
    "morgan": "^1.10.0",
    "oidc-client": "^1.9.0",
    "react": "^18.2.0",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-helmet": "^6.1.0",
    "react-router": "^6.21.1",
    "react-router-dom": "^6.21.1",
    "react-scripts": "^5.0.1",
    "reactstrap": "^9.2.1",
    "reactstrap-date-picker2": "^1.0.5",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/draft-js": "^0.11.1",
    "@types/draftjs-to-html": "^0.8.1",
    "@types/html-to-draftjs": "^1.4.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^18.2.46",
    "@types/react-bootstrap": "^0.32.35",
    "@types/react-dom": "^18.2.18",
    "@types/react-draft-wysiwyg": "^1.13.2",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-preset-razzle": "^4.2.18",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "immutable": "^3.8.2",
    "jest": "26.6.0",
    "jquery": "^3.4.1",
    "json-server": "^0.17.3",
    "postcss": "^8.4.6",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "razzle": "^4.2.18",
    "razzle-dev-utils": "^4.2.18",
    "sass": "^1.43.4",
    "stylelint": "^16.1.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "react-draft-wysiwyg": {
      "react": "^18.1.0",
      "react-dom": "^18.1.0"
    },
    "autoprefixer": "10.4.14"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/*.(spec|test).(ts|js)?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "./src/*/*.types.{ts,tsx}",
      "./src/index.tsx",
      "!src/service-worker.ts",
      "./src/serviceWorkerRegistration.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 12,
        "branches": 5,
        "functions": 6,
        "lines": 13
      }
    },
    "snapshotSerializers": [],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "src/(.*)$": "<rootDir>/src/$1",
      "\\.(gif|ttf|eot|svg|jpg|ico|jpeg|png|otf|webp|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "start": "rimraf ./build && react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.{js,ts,tsx,scss}",
    "lint": "eslint --fix src/**/*.{ts,tsx} -c .eslintrc.json",
    "stylelint": "stylelint **/*.scss --customSyntax postcss-scss --fix",
    "start-r": "react-scripts start",
    "test-r": "react-scripts test",
    "stubapi": "json-server --watch --port 4155 --routes routes.json db.json",
    "test:coverage": "jest --coverage --runInBand --watchAll=false",
    "isready": "npm run format && npm run lint && npm run stylelint && npm run test:coverage && npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "compilerOptions": {
    "jsx": "react",
    "noFallthroughCasesInSwitch": true
  }
}
